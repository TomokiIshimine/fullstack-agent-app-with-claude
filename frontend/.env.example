# Vite client environment variables
# API proxy target for development server (proxies /api/* requests)
VITE_API_PROXY=http://localhost:5000
# Optional: toggle mock mode during local development
VITE_ENABLE_MSW=false

# Logging configuration
# Log level: DEBUG, INFO, WARN, ERROR, SILENT
# Default: DEBUG in development, WARN in production
VITE_LOG_LEVEL=DEBUG

# Enable/disable API request/response logging
# Default: true in development, false in production
VITE_ENABLE_API_LOGGING=true
